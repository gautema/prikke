{
  "name": "prikke-waitlist",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:create": "wrangler d1 create prikke-waitlist",
    "db:init": "wrangler d1 execute prikke-waitlist --file=./schema.sql",
    "db:init:local": "wrangler d1 execute prikke-waitlist --local --file=./schema.sql",
    "db:list": "wrangler d1 execute prikke-waitlist --command='SELECT * FROM waitlist ORDER BY created_at DESC'"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240117.0",
    "typescript": "^5.3.3",
    "wrangler": "^3.24.0"
  }
}
